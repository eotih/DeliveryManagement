@*@model IEnumerable<ManaDeli.Models.ManaDeliEntities>*@
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">
    <!-- Title Page-->
    <title>Mana Deli</title>
    <!-- Fontfaces CSS-->
    <link href="~/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <!-- Bootstrap CSS-->
    <link href="~/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">
    <!-- Vendor CSS-->
    <link href="~/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">
    <!-- Main CSS-->
    <link href="~/css/theme.css" rel="stylesheet" media="all">
</head>
<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="#">
                            <h2>Mana Deli</h2>

                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
            <nav class="navbar-mobile">
                <div class="container-fluid">
                    <ul class="navbar-mobile__list list-unstyled">
                        <li>
                            <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Dashboard" }, Request.Url.Scheme)">
                                <i class="fas fa-tachometer-alt"></i>Trang Chủ
                            </a>
                        </li>
                        @if (Session["quyen"].ToString() == "nhanvien")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Order" }, Request.Url.Scheme)">
                                    <i class="fas fa-shopping-cart"></i>Tạo Đơn
                                </a>
                            </li>
                        }
                        @if (Session["quyen"].ToString() == "nhanvien")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "QuanLy" }, Request.Url.Scheme)">
                                    <i class="fas fa-bars"></i>Quản Lý Đơn Hàng
                                </a>
                            </li>
                        }

                        @if (Session["quyen"].ToString() == "nhanvien")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Search" }, Request.Url.Scheme)">
                                    <i class="fas fa-map-marker-alt"></i>Tra Cứu
                                </a>
                            </li>
                        }
                        @if (Session["quyen"].ToString() == "shipper")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Shipper" }, Request.Url.Scheme)">
                                    <i class="fas fa-map-marker-alt"></i>Xem đơn đã chọn
                                </a>
                            </li>
                        }
                        @if (Session["quyen"].ToString() == "shipper")
                        {
                            <li>
                                <a href="@Url.Action("Index", "Delivery")">
                                    <i class="fas fa-bars"></i>Nhận đơn
                                </a>
                            </li>
                        }

                        <li>
                            <a href="@Url.Action("UpdateUser", "Account")">
                                <i class="fas fa-users"></i>Cài đặt tài khoản
                            </a>
                        </li>
                        @if (Session["quyen"].ToString() == "admin")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Account" }, Request.Url.Scheme)">
                                    <i class="fas fa-users"></i>Quản lý người dùng
                                </a>
                            </li>
                        }
                    </ul>
                </div>
            </nav>
        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Dashboard" }, Request.Url.Scheme)">
                    <h2>Mana Deli</h2>
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li>
                            <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Dashboard" }, Request.Url.Scheme)">
                                <i class="fas fa-tachometer-alt"></i>Trang Chủ
                            </a>
                        </li>
                        @if (Session["quyen"].ToString() == "nhanvien")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Order" }, Request.Url.Scheme)">
                                    <i class="fas fa-shopping-cart"></i>Tạo Đơn
                                </a>
                            </li>
                        }
                        @if (Session["quyen"].ToString() == "nhanvien")
                        {
                            <li>
                                <a href="@Url.Action("Tatca", null, new { area = string.Empty, controller = "QuanLy" }, Request.Url.Scheme)">
                                    <i class="fas fa-bars"></i>Quản Lý Đơn Hàng
                                </a>
                            </li>
                        }

                        @if (Session["quyen"].ToString() == "shipper")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Shipper" }, Request.Url.Scheme)">
                                    <i class="fas fa-map-marker-alt"></i>Xem đơn đã chọn
                                </a>
                            </li>
                        }
                        @if (Session["quyen"].ToString() == "shipper")
                        {
                            <li>
                                <a href="@Url.Action("Index", "Delivery")">
                                    <i class="fas fa-bars"></i>Nhận đơn
                                </a>
                            </li>
                        }

                        <li>
                            <a href="@Url.Action("UpdateUser", "Account")">
                                <i class="fas fa-users"></i>Cài đặt tài khoản
                            </a>
                        </li>
                        @if (Session["quyen"].ToString() == "admin")
                        {
                            <li>
                                <a href="@Url.Action("Index", null, new { area = string.Empty, controller = "Account" }, Request.Url.Scheme)">
                                    <i class="fas fa-users"></i>Quản Lý Người Dùng
                                </a>
                            </li>
                        }

                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST">
                                <input class="au-input au-input--xl" type="text" name="search" placeholder="Search for datas &amp; reports..." />
                                <button class="au-btn--submit" type="submit">
                                    <i class="zmdi zmdi-search"></i>
                                </button>
                            </form>
                            <div class="header-button">
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="content">
                                            @if (Session["hoten"].ToString() != null)
                                            {
                                                <a>Wellcome: @Session["hoten"].ToString()</a>
                                            }
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="https://freepikpsd.com/wp-content/uploads/2019/10/user-png-image-9.png" alt="John Doe" />
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    @if (Session["hoten"].ToString() != null)
                                                    {
                                                        <a>Họ tên: @Session["hoten"].ToString()</a>
                                                    }
                                                </div>
                                                <div class="content">
                                                    @if (Session["email"].ToString() != null)
                                                    {
                                                        <a>Email: @Session["email"].ToString()</a>
                                                    }
                                                </div>
                                            </div>
                                            <div class="account-dropdown__footer">
                                                <a href="@Url.Action("UpdateUser","Account")">
                                                    <i class="zmdi zmdi-power"></i>Cập Nhật Tài Khoản
                                                </a>
                                            </div>
                                            <div class="account-dropdown__footer">
                                                <a href="@Url.Action("Logout","Account")">
                                                    <i class="zmdi zmdi-power"></i>Logout
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                @RenderBody()
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>
    </div>
    <!-- Jquery JS-->
    <script src="~/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/vendor/slick/slick.min.js"></script>
    <script src="~/vendor/wow/wow.min.js"></script>
    <script src="~/vendor/animsition/animsition.min.js"></script>
    <script src="~/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <script src="~/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/vendor/counter-up/jquery.counterup.min.js"></script>
    <script src="~/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/vendor/select2/select2.min.js"></script>
    <!-- Main JS-->
    <script src="~/js/main.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
<!-- end document-->
