@using ManaDeli.DAO
@using ManaDeli.Models
@{
    string key = Request.QueryString["key"];
    List<DONHANG> result = new List<DONHANG>();
}

<!DOCTYPE html>

<html lang="en">
<head>
    <script src="~/js/jquery-3.3.1.js"></script>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

    <title></title>
    <meta name="keywords" content="">
    <meta name="author" content="">
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">

    <link rel="shortcut icon" href="https://www.eotihvn.com/asset/1/images/icons/icon-48x48.png" />

    <link rel="stylesheet" href="https://www.eotihvn.com/asset/5/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="https://www.eotihvn.com/asset/5/css/style.css">
    <link rel="stylesheet" type="text/css" href="https://www.eotihvn.com/asset/5/css/accordiancss.css">
    <link rel="stylesheet" type="text/css" href="https://www.eotihvn.com/asset/5/css/owl.carousel.min.css">
    <link rel="stylesheet" type="text/css" href="https://www.eotihvn.com/asset/5/css/owl.theme.default.min.css">
    <link rel="stylesheet" type="text/css" href="https://www.eotihvn.com/asset/5/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="https://www.eotihvn.com/asset/5/css/1.css">
    <link rel="stylesheet" type="text/css" href="https://www.eotihvn.com/asset/5/css/css.css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700,800,900&display=swap&subset=vietnamese" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Cabin:200,300,400,500,600,700,800,900&display=swap&subset=vietnamese" rel="stylesheet">

    <script async="" src="https://www.eotihvn.com/asset/5/js/analytics.js"></script>
    <script src="https://www.eotihvn.com/asset/5/js/jquery.min.js"></script>
    <script src="https://www.eotihvn.com/asset/5/js/smoothscroll.js"></script>
    <script type="text/javascript">
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-96875843-3', 'auto');
        ga('send', 'pageview');
    </script>
    <style type="text/css">
        a.nav-link.learn-more-btn {
            white-space: nowrap;
        }

        a.navbar-brand-logo {
            font-weight: bold;
            font-size: 33px;
        }

        .strategy-card-icon-section img {
            display: block;
            margin-left: auto;
            margin-right: auto;
        }

        .footer-section {
            background-color: #fbfbfb;
        }

        span.text pre {
            color: #191847;
            font-family: Cabin;
            font-size: 16px;
            line-height: 24px;
        }

        .block-23 ul li {
            margin-bottom: 0;
        }
    </style>
</head>
<body>

    <nav id="navbar" class="navbar fixed-top navbar-expand-lg navbar-header navbar-mobile">
        <div class="navbar-container container">

            <div class="navbar-brand">
                <a class="navbar-brand-logo" href="#">
                    <img src="~/images/manadeli.png" />
                </a>
            </div>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-end" id="navbarNav">
                <ul class="navbar-nav menu-navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" href="">
                            <p class="nav-link-menu">Trang chủ</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#services">
                            <p class="nav-link-menu">Dịch vụ</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#contact">
                            <p class="nav-link-menu">Liên hệ</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="@Url.Action("Index", null, new { area = string.Empty, controller = "Search" }, Request.Url.Scheme)">
                            <p class="nav-link-menu">Tra cứu mã vận đơn</p>
                        </a>
                    </li>
                </ul>
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link learn-more-btn" href="@Url.Action("Register", null, new { area = string.Empty, controller = "Account" }, Request.Url.Scheme)">Đăng ký làm shipper</a>
                    </li>
                </ul>


            </div>
        </div>
    </nav>

    <div id="top"></div>

    <div class="wrapper">
        <div class="header">
            <div class="container header-container">
                <div class="col-lg-6 header-img-section">
                    <img src="https://www.eotihvn.com/asset/5/images/header.png">
                </div>
                <div class="col-lg-6 header-title-section">
                    <h1 class="header-title">Dịch vụ giao hàng chuyên nghiệp  <strong class="text-primary"></strong></h1>
                    <p class="header-title-text">Dịch vụ giao hàng thương mại điện tử</p>
                    <div class="learn-more-btn-section">
                        <a class="nav-link learn-more-btn" href="@Url.Action("Login", null, new { area = string.Empty, controller = "Account" }, Request.Url.Scheme)">Đăng nhập</a>
                    </div>
                </div>
            </div>
        </div>

        <div id="courses"></div>

        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="card-header">Tra Cứu Thông Tin Đơn Hàng</div>
                            <div class="card-body">
                                <form name="timkiem">
                                    <div class="form-group">
                                        <label for="cc-payment" class="control-label mb-1">Mã vận đơn </label>
                                        <input name="key" type="text" class="form-control" aria-required="true" aria-invalid="false" placeholder="Nhập mã đơn để tra cứu">
                                    </div>
                                    <div>
                                        <button name="submit" type="submit" class="btn btn-lg btn-info btn-block">
                                            <i class="fa fa-spinner"></i>
                                            <span>Tra Cứu</span>
                                        </button>
                                        <div id="contentHolder"></div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <h2 class="title-1 m-b-25">Thông Tin Đơn Hàng</h2>
                        <div class="table-responsive table--no-card m-b-40">
                            <table class="table table-borderless table-striped table-earning">
                                <thead>
                                    <tr>
                                        <th>Mã vận đơn</th>
                                        <th>Trọng lượng(Gram)</th>
                                        <th>Dịch vụ</th>
                                        <th>Trạng Thái</th>
                                    </tr>
                                    @{

                                        if (key == null)
                                        {
                                            <h3>Vui lòng nhập mã vận đơn</h3>
                                        }
                                        else
                                        {
                                            result = new DonHang().SearchByKey(key.ToString());
                                            foreach (var item in result)
                                            {
                                                <tr>
                                                    <td>@item.madonhang</td>
                                                    <td>@item.cannang</td>
                                                    <td>@item.dichvu</td>
                                                    <td>@item.trangthai</td>
                                                </tr>
                                            }
                                        }

                                    }

                                </thead>
                                <tbody class="tbody"></tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>

        </div>
        
    </div>

    <script src="https://www.eotihvn.com/asset/5/js/jquery.min(1).js"></script>
    <script src="https://www.eotihvn.com/asset/5/js/popper.min.js"></script>
    <script src="https://www.eotihvn.com/asset/5/js/bootstrap.min.js"></script>
    <script src="https://www.eotihvn.com/asset/5/js/owl.carousel.js"></script>
    <script src="https://www.eotihvn.com/asset/5/js/accordianscript.js"></script>

    <script type="text/javascript">
        $(window).scroll(function () {
            if ($(this).scrollTop() > 20) {
                $('#navbar').addClass('header-scrolled');
            } else {
                $('#navbar').removeClass('header-scrolled');
            }
        });
    </script>
    <script>
        $(document).ready(function () {
            $(".owl-vertical-slider").owlCarousel({
                items: 1,
                loop: true,
                nav: true,
                autoplay: true,
                autoplayTimeout: 5000,
                autoplayHoverPause: false,
                responsiveClass: true,
                animateIn: 'fadeIn',
                animateOut: 'fadeOut',
                responsive: {
                    0: {
                        items: 1
                    },
                    600: {
                        items: 1
                    },
                    1000: {
                        items: 1
                    }
                }
            });
        });
    </script>
    <script>
        $(document).ready(function () {
            $(".owl-reviews-slider").owlCarousel({
                items: 1,
                loop: true,
                nav: true,
                autoplay: true,
                autoplayTimeout: 5000,
                autoplayHoverPause: false,
                responsiveClass: true,
                responsive: {
                    0: {
                        items: 1
                    },
                    600: {
                        items: 1
                    },
                    1000: {
                        items: 1
                    }
                }
            });
        });
    </script>
</body>
</html>

